	<select id="selectBuskerByNo" parameterType="int" resultType="busker">
		select *
		  from tb_busker
		 where busker_no = #{buskerNo}
	</select>
	
	<select id="selectBusker" resultType="busker">
		select *
		  from tb_busker
		 limit 15
	</select>
